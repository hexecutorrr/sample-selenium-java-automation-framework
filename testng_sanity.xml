			<class name="com.javaautomation.testcases.OrderPageTest" />
			<class name="com.javaautomation.testcases.ProductPageTest" />
			<class name="com.javaautomation.testcases.CreateAccountPageTest" />
			<class name="com.javaautomation.testcases.SearchResultPageTest" />
